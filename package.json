{
  "name": "dayler-ui",
  "version": "2.0.0",
  "main": "./lib/index.min.js",
  "scripts": {
    "dist": "webpack --config ./webpack/index.js",
    "lint": "eslint .",
    "lib": "webpack --config ./webpack/webpack.lib.js",
    "start": "node ./server.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iknpx/dayler-ui.git"
  },
  "keywords": [
    "buttons",
    "checkbox",
    "date-picker",
    "feather",
    "image-crop",
    "input",
    "panels",
    "radio",
    "react",
    "snackbar",
    "stylus",
    "switcher",
    "tabs",
    "typings",
    "ui",
    "ui-components",
    "webpack"
  ],
  "author": "Iknpx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iknpx/dayler-ui/issues"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/app/**/*.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|styl)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/__tests__/setup/index.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "__tests__/setup"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 10,
        "lines": 10,
        "statements": 10
      }
    }
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-feather": "^1.1.0",
    "react-router-dom": "^4.2.2",
    "react-syntax-highlighter": "^8.0.1",
    "s-grid": "^1.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-dayler": "^1.0.2",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "ghooks": "^2.0.4",
    "gulp": "^3.9.1",
    "gulp-sftp": "^0.1.5",
    "helmet": "^3.12.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.1.0",
    "jest-mock-axios": "^2.1.11",
    "postcss-loader": "^2.1.5",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^4.2.0",
    "rupture": "^0.7.1",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.2",
    "webpack-cli": "^2.1.3",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.2"
  }
}
